{
  "name": "PRs",
  "enforcement": "active",
  "conditions": {
    "ref_name": {
      "exclude": [
        "refs/heads/main"
      ],
      "include": []
    }
  },
  "rules": [
    {
      "type": "copilot_code_review",
      "parameters": {
        "review_on_push": true,
        "review_draft_pull_requests": false
      }
    },
    {
      "type": "copilot_code_review_analysis_tools",
      "parameters": {
        "tools": [
          {
            "name": "CodeQL"
          },
          {
            "name": "ESLint"
          }
        ]
      }
    },
    {
      "type": "code_quality",
      "parameters": {
        "severity": "errors"
      }
    }
  ],
  "bypass_actors": [
    {
      "actor_id": null,
      "actor_type": "OrganizationAdmin",
      "bypass_mode": "always"
    },
    {
      "actor_id": 5,
      "actor_type": "RepositoryRole",
      "bypass_mode": "always"
    },
    {
      "actor_id": 2740,
      "actor_type": "Integration",
      "bypass_mode": "always"
    },
    {
      "actor_id": 29110,
      "actor_type": "Integration",
      "bypass_mode": "always"
    }
  ]
}